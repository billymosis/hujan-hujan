generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native"]
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Station {
  station_number Int       @id @default(autoincrement())
  station_name   String?
  latitude       Float?
  longitude      Float?
  elevation      Float?
  Weather        Weather[]
}

model Weather {
  id             Int      @id @default(autoincrement())
  ddd_car        String?
  Tanggal        String?
  station_number Int?
  Tn             Float?
  Tx             Float?
  Tavg           Float?
  RH_avg         Float?
  RR             Float?
  ss             Float?
  ff_x           Float?
  ddd_x          Float?
  ff_avg         Float?
  Station        Station? @relation(fields: [station_number], references: [station_number], onDelete: NoAction, onUpdate: NoAction)
}
